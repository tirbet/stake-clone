model Currency {
  id        String   @id @default(uuid())
  code      String   @unique
  name      String
  symbol    String?
  useRate   Decimal  @default(0.0)
  decimals  Int      @default(2)
  isActive  Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  wallets   Wallet[]

  @@map("currency")
}
