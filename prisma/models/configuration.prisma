model Configuration {
  id        String            @id @default(uuid())
  type      ConfigurationType
  value     Json
  isActive  Boolean           @default(true)
  createdAt DateTime          @default(now())
  updatedAt DateTime          @updatedAt

  @@map("configuration")
}

enum ConfigurationType {
  Jwt
  ApiKey
  WebSite
  Domain
  Payment
  Smtp
  AuthAttempt
}
